{
  "name": "megascore-monorepo",
  "private": true,
  "version": "0.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "typecheck": "pnpm -r --parallel typecheck",
    "web:dev": "pnpm --filter @megascore/web dev",
    "web:build": "pnpm --filter @megascore/web build",
    "web:start": "pnpm --filter @megascore/web start",
    "web:lint": "pnpm --filter @megascore/web lint",
    "web:typecheck": "pnpm --filter @megascore/web typecheck",
    "web:gen:supabase:types": "pnpm --filter @megascore/web gen:supabase:types",
    "hh:dev": "pnpm --filter @megascore/hardhat dev",
    "hh:compile": "pnpm --filter @megascore/hardhat compile",
    "hh:test:solidity": "pnpm --filter @megascore/hardhat test:solidity",
    "hh:deploy:local": "pnpm --filter @megascore/hardhat deploy:local && pnpm --filter @megascore/hardhat abi:export && pnpm --filter @megascore/hardhat addresses:export && pnpm --filter @megascore/hardhat send:eth && pnpm --filter @megascore/hardhat transfer:ownership",
    "hh:abi:export": "pnpm --filter @megascore/hardhat abi:export",
    "hh:addresses:export": "pnpm --filter @megascore/hardhat addresses:export",
    "hh:contracts:build": "pnpm hh:compile && pnpm hh:abi:export",
    "hh:send:eth": "pnpm --filter @megascore/hardhat send:eth",
    "hh:transfer:ownership": "pnpm --filter @megascore/hardhat transfer:ownership"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "packageManager": "pnpm@9.12.0",
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0"
  }
}